{
  "manifest_version": 2,
  "name": "Jacket?",
  "version": "1.0.0",
  "icons": {
    "128": "Jacket-icon.png"
  },
  "description": "A little plugin that lets you know if you need a jacket!",
  "browser_action": {
    "default_icon": "Jacket-icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "geolocation"
  ]
}
